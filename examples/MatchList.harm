module MatchList

import IO.Console as Console
import Data.List as List

fun main =
    let list = List.filter (
            fun(x: int) -> bool = x % 2 == 0,
            List.range (1, 10)) in
    let reversed = List.reverse(list) in
    Console.println (List.map (
            fun(x: int) -> int = x * x,
            reversed))